{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["*.ts", "src/**/*", "src/prisma", "controllers", "src/prisma", "src/routes/teamMemberRoutes.ts", "src/prisma/client.ts"]
}
